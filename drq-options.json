{
  "loggingOptions": {
    "logLevel": "trace",
    "useLevelPrefixes": true,
    "useConsoleTrace": false
  },

  "stageHandlingOptions": {
    "streamNameStageSeparator": "_",
    "resourceNameStageSeparator": "_",
    "extractInCase": "lower",
    "injectInCase": "upper"
  },

  "streamProcessingOptions": {
    "streamType": "kinesis",
    "taskTrackingName": "drqTaskTracking",
    "timeoutAtPercentageOfRemainingTime": 0.9,
    "maxNumberOfAttempts": 10,
    "deadRecordQueueName": "DeadRecordQueue",
    "deadMessageQueueName": "DeadMessageQueue",
    "kinesisOptions": {
      "maxRetries": 1
    },
    "dynamoDBDocClientOptions": {
      "maxRetries": 1
    },
    "deadRecordTableName": "DeadRecord"
  }

}